optima.raw=importData(path)

### Check that the sheet names are correct:
### Total, Warranties, OOW, Bounce
for(i in 1:length(optima.raw)) print(names(optima.raw[[i]]))

lookup.w=c("ServiceNo", "Retailer", "IMEI", "Serial n", "Symptom", "Service type", "Purchase Date", "Repair Received", "Repair Completed")
optima.warranty=formatMain(optima.raw, lookup=lookup.w, sheet="Warranty")

lookup.o=lookup.w
optima.oow=formatMain(optima.raw, sheet="OOW", lookup=lookup.o)
optima.spares=formatSpares(optima.raw)

optimaData=merge(optima.spares, optima.warranty, by="ServiceNo")
write.table(optima.warranty, "Optima Warranty.csv", sep=";")
